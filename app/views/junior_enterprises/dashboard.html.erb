<%= render 'layouts/headmenu' %>
<div id="layout-container">
  <div id="main">  
      <div class="head-title"></div>
      <div class="margin-container">
        <div class="scrollable wrapper">
          <div class="row">
            <div class="col-md-12">
              <div class="page-heading">
              </div>
            </div>
          </div>
          <div class="row">
                    <div class="profile-info col-lg-9 col-lg-offset-1">                        
                        <div class="panel">
                            <div class="bio-graph-heading">
                              <h3> <%= @current_user.junior_enterprise.phrase %> </h3>
                            </div>
                            <div class="panel-body bio-graph-info">
                                <h1><%= @current_user.junior_enterprise.name %></h1>
                                <div class="row">
                                    <% unless @current_user.junior_enterprise.course.blank? %>
                                      <div class="bio-row">
                                          <p><span>Curso: </span> <%= @current_user.junior_enterprise.course %></p>
                                      </div>
                                    <% end %>
                                    <% unless @current_user.junior_enterprise.area.blank? %>
                                      <div class="bio-row">
                                          <p><span>Area: </span> <%= @current_user.junior_enterprise.area %></p>
                                      </div>
                                    <% end %>
                                    <% unless @current_user.junior_enterprise.site.blank? %>
                                      <div class="bio-row">
                                          <p><span>Site: </span> <%= @current_user.junior_enterprise.site %></p>
                                      </div>
                                    <% end %>
                                    <% unless @current_user.junior_enterprise.phone.blank? %>
                                      <div class="bio-row">
                                          <p><span>Telefone: </span> <%= @current_user.junior_enterprise.phone %></p>
                                      </div>
                                    <% end %>
                                    <% unless @current_user.junior_enterprise.youtube.blank? %>
                                      <div class="bio-row">
                                          <p><span>Youtube: </span> <%= @current_user.junior_enterprise.youtube %></p>
                                      </div>
                                    <% end %>
                                    <% unless @current_user.junior_enterprise.facebook.blank? %>
                                      <div class="bio-row">
                                          <p><span>Facebook: </span> <%= @current_user.junior_enterprise.facebook %></p>
                                      </div>
                                    <% end %>
                                    <% unless @current_user.junior_enterprise.city.blank? %>
                                      <div class="bio-row">
                                          <p><span>Cidade: </span> <%= @current_user.junior_enterprise.city %></p>
                                      </div>
                                    <% end %>
                                    <% unless @current_user.junior_enterprise.state.blank? %>
                                      <div class="bio-row">
                                          <p><span>Estado: </span> <%= @current_user.junior_enterprise.state %></p>
                                      </div>
                                    <% end %>
                                    <% unless @current_user.junior_enterprise.address.blank? %>
                                      <div class="bio-row">
                                          <p><span>Endere√ßo: </span> <%= @current_user.junior_enterprise.address %></p>
                                      </div>
                                    <% end %>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>   
        </div>
      </div>
    </div>
</div>
