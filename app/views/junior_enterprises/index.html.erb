<h1>Listing junior_enterprises</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Logo</th>
      <th>Description</th>
      <th>Phrase</th>
      <th>Site</th>
      <th>Phone</th>
      <th>City</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @junior_enterprises.each do |junior_enterprise| %>
      <tr>
        <td><%= junior_enterprise.name %></td>
        <td><%= junior_enterprise.logo %></td>
        <td><%= junior_enterprise.description %></td>
        <td><%= junior_enterprise.phrase %></td>
        <td><%= junior_enterprise.site %></td>
        <td><%= junior_enterprise.phone %></td>
        <td><%= junior_enterprise.city %></td>
        <td><%= junior_enterprise.state %></td>
        <td><%= link_to 'Show', junior_enterprise %></td>
        <td><%= link_to 'Edit', edit_junior_enterprise_path(junior_enterprise) %></td>
        <td><%= link_to 'Destroy', junior_enterprise, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Junior enterprise', new_junior_enterprise_path %>
