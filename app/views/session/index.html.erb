<div class="intro-header">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="intro-message">
          <h1>Vitrine de EJ</h1>
          <h3>Todas as soluções que você procura!</h3>
          <hr class="intro-divider">
          <ul class="list-inline intro-social-buttons">
            <li>
              <%= link_to "/find", :class=>"btn btn-success btn-lg" do %>
                <span class="network-name">Encontrar uma Solução</span>
              <% end %>
            </li>
            <li>
              <%= link_to log_in_path, :class=>"btn btn-warning btn-lg" do %>
                <span class="network-name">Login</span>
              <% end %>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>